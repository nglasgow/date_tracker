<h2>New Tracker</h2>
<%= form_for(@tracker) do |f| %>
<%= f.error_messages %>
  <p>
    <%= f.label :date %><br />
    <%= f.text_field :date %>
  </p>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  <%= f.submit "Add Date" %>
<% end %>

<%= link_to "Back to List", trackers_path %>
